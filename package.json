  "scripts": {
    "build": "npm run build:next",
    "build:analyze": "ANALYZE=true npm run build:next",
    "build:clean": "rm -rf .next",
    "build:next": "npm run build:clean && next build",
    "dev": "next dev",
    "dev:test": "jest --watch --maxWorkers=1",
    "format": "prettier --write . && npm run lint:fix",
    "lint": "next lint",
    "lint:fix": "npm run lint --fix",
    "nuke": "rm -rf .next dist node_modules",
    "prepare": "husky install",
    "sort": "npx sort-package-json './package.json'",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:updateSnapshots": "jest --updateSnapshot",
    "test:watch": "jest --watch --maxWorkers=1",
    "cypress:open": "cypress open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --exit-zero-on-changes"
  },
